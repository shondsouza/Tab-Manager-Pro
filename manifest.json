{
  "manifest_version": 3,
  "name": "Tab Manager Pro",
  "version": "1.0",
  "description": "Organize your tabs into groups and boost productivity",
  "permissions": [
    "tabs",
    "storage",
    "activeTab"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Tab Manager Pro"
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}